{
	"manifest_version": 2,
    "version": "0.2",
    "name": "Beppify",
    "description": "Sostituisce i nomi dei politici con i soprannomi inventati da Beppe Grillo.",
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["content_script.js"],
            "run_at": "document_end"
        }],
    "icons": { "16": "icon16.png",
               "48": "icon48.png",
              "128": "icon128.png" }
}
